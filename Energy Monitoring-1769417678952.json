          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "-- Quick test: replace the dates below with your range (UTC)\r\nWITH daily_last AS (\r\n    SELECT DISTINCT ON (s.devicename, q.quantityname, local_day)\r\n        DATE(d.timestamputc AT TIME ZONE 'Europe/Skopje') AS local_day,\r\n        s.devicename,\r\n        q.quantityname,\r\n        d.value\r\n    FROM datalog2 d\r\n    JOIN sources s ON d.sourceid = s.sourceid\r\n    JOIN quantities q ON d.quantityid = q.quantityid\r\n    WHERE\r\n        q.quantityname IN ('Active_Energy_Delivered', 'Active_Energy_Received')\r\n        AND s.devicename IN ('Supply Fan','Exhaust Fan','Heat Recovery Motor','Pump')\r\n        AND d.timestamputc >= TIMESTAMPTZ '2026-01-01 00:00:00+00'\r\n        AND d.timestamputc <  TIMESTAMPTZ '2026-01-24 00:00:00+00'\r\n    ORDER BY\r\n        s.devicename, q.quantityname, local_day, d.timestamputc DESC\r\n),\r\ndaily_diff AS (\r\n    SELECT\r\n        local_day AS day,\r\n        devicename,\r\n        quantityname,\r\n        GREATEST(\r\n            value - LAG(value) OVER (\r\n                PARTITION BY devicename, quantityname\r\n                ORDER BY local_day\r\n            ),\r\n            0\r\n        ) AS kwh\r\n    FROM daily_last\r\n)\r\nSELECT\r\n    day AS time,\r\n    devicename || ' - ' || quantityname || ' (Daily â‚¬)' AS metric,\r\n    kwh * 0.12::numeric AS value\r\nFROM daily_diff\r\nWHERE kwh IS NOT NULL\r\nORDER BY time, metric;\r\n\r\n\r\n\r\n",
          "refId": "A",

      "targets": [
        {
          "dataset": "iiot_gateway",
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH daily_last AS (\r\n    SELECT DISTINCT ON (s.devicename, q.quantityname, DATE(d.timestamputc))\r\n        DATE(d.timestamputc) AS day,\r\n        s.devicename,\r\n        q.quantityname,\r\n        d.value\r\n    FROM datalog2 d\r\n    JOIN sources s ON d.sourceid = s.sourceid\r\n    JOIN quantities q ON d.quantityid = q.quantityid\r\n    WHERE\r\n        q.quantityname IN (\r\n            'Active_Energy_Delivered',\r\n            'Active_Energy_Received'\r\n        )\r\n        AND s.devicename IN (\r\n            'Supply Fan',\r\n            'Exhaust Fan',\r\n            'Heat Recovery Motor',\r\n            'Pump'\r\n        )\r\n        AND $__timeFilter(d.timestamputc)\r\n    ORDER BY\r\n        s.devicename,\r\n        q.quantityname,\r\n        DATE(d.timestamputc),\r\n        d.timestamputc DESC\r\n),\r\ndaily_diff AS (\r\n    SELECT\r\n        day,\r\n        devicename,\r\n        quantityname,\r\n        value - LAG(value) OVER (\r\n            PARTITION BY devicename, quantityname\r\n            ORDER BY day\r\n        ) AS daily_energy\r\n    FROM daily_last\r\n),\r\naggregated AS (\r\n    SELECT\r\n        day,\r\n        quantityname,\r\n        SUM(daily_energy) AS energy\r\n    FROM daily_diff\r\n    WHERE daily_energy IS NOT NULL\r\n    GROUP BY day, quantityname\r\n)\r\nSELECT\r\n    day AS time,\r\n    quantityname AS metric,\r\n    energy\r\nFROM aggregated\r\n\r\nUNION ALL\r\n\r\nSELECT\r\n    day AS time,\r\n    'Net_Energy' AS metric,\r\n    SUM(\r\n        CASE\r\n            WHEN quantityname = 'Active_Energy_Delivered' THEN energy\r\n            WHEN quantityname = 'Active_Energy_Received'  THEN -energy\r\n        END\r\n    ) AS energy\r\nFROM aggregated\r\nGROUP BY day\r\n\r\nORDER BY time, metric;\r\n\r\n",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
